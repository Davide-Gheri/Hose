{
  "appName": "Hose",
  "rootId": "root",
  "persist": {
    "dbName": "hose"
  },
  "i18n": {
    "backendUrl": "http://localhost:5000/locales",
    "defaultLanguage": "en",
    "fallbackLanguage": "en",
    "currentLanguageKey": "hose_i18n_current_language",
    "cache": {
      "storeName": "hose_i18n"
    }
  },
  "redux": {
    "devtools": true,
    "persist": [
      "themes",
      "dashboard"
    ]
  },
  "logger": {
    "enabled": true,
    "loggers": {
      "redux": false,
      "fetch": true,
      "io": true,
      "cache": true
    }
  },
  "fetch": {
    "apiUrl": "http://localhost:5000/api",
    "debug": true,
    "cache": true
  },
  "io": {
    "wsUrl": "http://localhost:8888",
    "debug": true
  }
}
